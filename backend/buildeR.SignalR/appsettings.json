{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "Kafka": {
    "GroupId": "logs-consumers-group",
    "BootstrapServers": "localhost:9092"
  },
  "AllowedHosts": "*"
}
